    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "~4.5.4",
    "vite": "^2.7.12",
    "vue-tsc": "^0.30.2"
